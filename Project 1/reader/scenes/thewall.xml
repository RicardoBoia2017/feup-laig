      </DESCENDANTS>

    </NODE>

    <NODE id="block16">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0" z="3"/>
      <SCALE sx="5" sy="30" sz="5"/>

      <DESCENDANTS>
