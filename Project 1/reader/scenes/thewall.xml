      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="block9">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="165" y="0" z="3"/>
      <SCALE sx="35" sy="45" sz="2"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="block10">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0" z="4"/>
      <SCALE sx="200" sy="35" sz="2"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="block11">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0" z="6"/>
      <SCALE sx="50" sy="27" sz="1"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="block12">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="50" y="0" z="6"/>
      <SCALE sx="80" sy="30" sz="2"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="block13">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="130" y="0" z="6"/>
      <SCALE sx="70" sy="20" sz="1"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <!-- Outros blocos -->

    <NODE id="block14">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0" z="0"/>
      <SCALE sx="25" sy="5" sz="14"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="block15">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0" z="0"/>
