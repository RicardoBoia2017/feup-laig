      </DESCENDANTS>

    </NODE>

    <!-- The elevator -->
    <NODE id="elevator">
      <MATERIAL id="null"/>
      <TEXTURE id="wood"/>

      <TRANSLATION x="98" y="0" z="8"/>

      <DESCENDANTS>
        <NODEREF id="elevatorStructure"/>
        <NODEREF id="elevatorBox"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="elevatorStructure">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="elevatorStructure1"/>
        <NODEREF id="elevatorStructure2"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="elevatorStructure1">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="4" y="0" z="0"/>
      <SCALE sx="1" sy="70" sz="1"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="elevatorStructure2">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <SCALE sx="1" sy="70" sz="1"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="elevatorBox">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="30" z="0"/>
      <SCALE sx="5" sy="5" sz="3"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <!-- The watch -->
    <NODE id="thewatch">
      <MATERIAL id="null"/>
      <TEXTURE id="wood"/>

      <TRANSLATION x="98" y="70" z="-2"/>

      <DESCENDANTS>
        <NODEREF id="step"/>
        <NODEREF id="hall"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="step">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0" z="0"/>
      <SCALE sx="5" sy="1" sz="11"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="hall">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="-2" y="0" z="-9"/>
      <SCALE sx="9" sy="3" sz="9"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <!-- Cube -->
    <NODE id="cube">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="faceCube1"/>
        <NODEREF id="faceCube2"/>
        <NODEREF id="faceCube3"/>
        <NODEREF id="faceCube4"/>
        <NODEREF id="faceCube5"/>
        <NODEREF id="faceCube6"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="faceCube1">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0" z="1"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 1 1 0"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="faceCube2">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0" z="0"/>
      <ROTATION axis="y" angle="-90"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 1 1 0"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="faceCube3">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="1" y="0" z="0"/>
      <ROTATION axis="y" angle="180"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 1 1 0"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="faceCube4">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="1" y="0" z="1"/>
      <ROTATION axis="y" angle="90"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 1 1 0"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="faceCube5">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="1" z="1"/>
      <ROTATION axis="x" angle="-90"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 1 1 0"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="faceCube6">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0" z="0"/>
      <ROTATION axis="x" angle="90"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 1 1 0"/>
      </DESCENDANTS>

    </NODE>

  </NODES>

</SCENE>
