<SCENE>

  <!-- Initial values -->
  <INITIALS>
    <frustum near="0.1" far="500"/>
    <translation x="-3" y="1.5" z="0" /> 
    <rotation axis="x" angle="0"/>
    <rotation axis="y" angle="0"/>
    <rotation axis="z" angle="0"/>
    <scale sx="0.05" sy="0.05" sz="0.05"/>
    <reference length="5"/>
  </INITIALS>

  <!-- Illumination and clear colour -->
  <ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1"/>
    <background r="0" g="0" b="0" a="1"/>
  </ILLUMINATION>

  <!-- Lights -->
  <LIGHTS>
    <LIGHT id="light0">
      <enable value="1"/>
      <position x="0" y="50" z="10" w="1"/>
      <ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0"/>
      <specular r="1.0" g="1.0" b="1.0" a="1.0"/>
    </LIGHT>

    <LIGHT id="light1">
      <enable value="1"/>
      <position x="150" y="50" z="10" w="1"/>
      <ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0"/>
      <specular r="1.0" g="1.0" b="1.0" a="1.0"/>
    </LIGHT>

    <LIGHT id="light2">
      <enable value="1"/>
      <position x="0" y="50" z="30" w="1"/>
      <ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0"/>
      <specular r="1.0" g="1.0" b="1.0" a="1.0"/>
    </LIGHT>

    <LIGHT id="light3">
      <enable value="1"/>
      <position x="150" y="50" z="30" w="1"/>
      <ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
      <diffuse r="0.6" g="0.6" b="0.6" a="1.0"/>
      <specular r="0.6" g="0.6" b="0.6" a="1.0"/>
    </LIGHT>

  </LIGHTS>

  <!-- Textures -->
  <TEXTURES>
    <!-- Tree trunk -->
    <TEXTURE id="trunk">
      <file path="images/trunk-image.jpg"/>
      <amplif_factor s="2" t="2"/>
    </TEXTURE>

  </TEXTURES>

  <MATERIALS>

    <!-- Green Material -->
    <MATERIAL id="greenMaterial">
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />
        <diffuse r="0.33" g="0.42" b="0.18" a="1" />
        <ambient r="0.0" g="0.0" b="0.0" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Brown Material -->
    <MATERIAL id="brownMaterial">
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />
        <diffuse r="0.4" g="0.19" b="0.03" a="1" />
        <ambient r="0.0" g="0.0" b="0.0" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Dark Brown Material -->
    <MATERIAL id="darkBrownMaterial">
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />
        <diffuse r="0.3" g="0.09" b="0.00" a="1" />
        <ambient r="0.0" g="0.0" b="0.0" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- A default material -->
    <MATERIAL id="defaultMaterial">
      <shininess value="1"/>
      <specular r="0.0" g="0.0" b="0.0" a="1"/>
      <diffuse r="0.4" g="0.4" b="0.4" a="1"/>
      <ambient r="0.4" g="0.4" b="0.4" a="1"/>
      <emission r="0.0" g="0.0" b="0.0" a="1"/>
    </MATERIAL>

  </MATERIALS>

  <!-- The leaves -->
  <LEAVES></LEAVES>

  <!-- The nodes -->
  <NODES>

    <ROOT id="root"/>

    <!-- The root node -->
    <NODE id="root">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="floor"/>
        <NODEREF id="wall"/>
        <NODEREF id="forest"/>
        <NODEREF id="elevator"/>
        <NODEREF id="town"/>
      </DESCENDANTS>

    </NODE>

    <!-- The floor -->
    <NODE id="floor">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <SCALE sx="200" sy="1" sz="100"/>
      <TRANSLATION x="0" y="0" z="1"/>
      <ROTATION axis="x" angle="-90"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 1 1 0"/>
      </DESCENDANTS>

    </NODE>

    <!-- The wall -->
    <NODE id="wall">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="bloco1"/>
        <NODEREF id="bloco2"/>
        <NODEREF id="bloco3"/>
        <NODEREF id="bloco4"/>
        <NODEREF id="bloco5"/>
        <NODEREF id="bloco6"/>
        <NODEREF id="bloco7"/>
        <NODEREF id="bloco8"/>
        <NODEREF id="bloco9"/>
        <NODEREF id="bloco10"/>
        <NODEREF id="bloco11"/>
        <NODEREF id="bloco12"/>
        <NODEREF id="bloco13"/>
      </DESCENDANTS>

    </NODE>

    <!-- The forest -->
    <NODE id="forest">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="piramide1"/>
      </DESCENDANTS>

    </NODE>

    <!-- The wall -->
    <NODE id="bloco1">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0" z="-10"/>
      <SCALE sx="25" sy="5" sz="50"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="bloco2">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="5" y="5" z="0"/>
      <SCALE sx="20" sy="20" sz="35"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="bloco3">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="10" y="25" z="-5"/>
      <SCALE sx="25" sy="10" sz="30"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="bloco4">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="25" y="0" z="-5"/>
      <SCALE sx="40" sy="15" sz="35"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="bloco5">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="25" y="15" z="-2"/>
      <SCALE sx="40" sy="10" sz="30"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="bloco6">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="35" y="25" z="0"/>
      <SCALE sx="25" sy="20" sz="20"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="bloco7">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="50" y="0" z="-10"/>
      <SCALE sx="60" sy="50" sz="20"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="bloco8">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="100" y="0" z="-20"/>
      <SCALE sx="20" sy="45" sz="40"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="bloco9">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="120" y="0" z="0"/>
      <SCALE sx="30" sy="30" sz="15"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="bloco10">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="140" y="0" z="-20"/>
      <SCALE sx="30" sy="20" sz="45"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="bloco11">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="120" y="0" z="-15"/>
      <SCALE sx="50" sy="40" sz="15"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="bloco12">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="170" y="0" z="-15"/>
      <SCALE sx="20" sy="15" sz="35"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="bloco13">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="170" y="15" z="-10"/>
      <SCALE sx="10" sy="10" sz="30"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <!-- The elevator -->
    <NODE id="elevator">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="elevatorStructure"/>
        <NODEREF id="elevatorBox"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="elevatorStructure">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="elevatorStructure1"/>
        <NODEREF id="elevatorStructure2"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="elevatorStructure1">
      <MATERIAL id="brownMaterial"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="77" y="0" z="10"/>
      <SCALE sx="1" sy="50" sz="1"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="elevatorStructure2">
      <MATERIAL id="brownMaterial"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="80" y="0" z="10"/>
      <SCALE sx="1" sy="50" sz="1"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="elevatorBox">
      <MATERIAL id="brownMaterial"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="77" y="30" z="10"/>
      <SCALE sx="4" sy="5" sz="3"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <!--Town -->

    <NODE id="town">
      <MATERIAL id="darkBrownMaterial"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="house1"/>
        <NODEREF id="house2"/>
        <NODEREF id="house3"/>
        <NODEREF id="house4"/>
        <NODEREF id="house5"/>
        <NODEREF id="house6"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="house1">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="77" y="0" z="20"/>
      <SCALE sx="4" sy="7" sz="5"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="house2">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="85" y="0" z="20"/>
      <SCALE sx="4" sy="3" sz="5"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="house3">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="70" y="0" z="30"/>
      <SCALE sx="6" sy="8" sz="4"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="house4">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="80" y="0" z="27"/>
      <SCALE sx="4" sy="4" sz="4"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="house5">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="90" y="0" z="35"/>
      <SCALE sx="2" sy="7" sz="7"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="house6">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="95" y="0" z="30"/>
      <SCALE sx="4" sy="7" sz="5"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>

    </NODE>



    <!-- Cube -->
    <NODE id="cube">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="faceCubo1"/>
        <NODEREF id="faceCubo2"/>
        <NODEREF id="faceCubo3"/>
        <NODEREF id="faceCubo4"/>
        <NODEREF id="faceCubo5"/>
        <NODEREF id="faceCubo6"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="faceCubo1">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0" z="1"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 1 1 0"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="faceCubo2">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0" z="0"/>
      <ROTATION axis="y" angle="-90"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 1 1 0"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="faceCubo3">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="1" y="0" z="0"/>
      <ROTATION axis="y" angle="180"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 1 1 0"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="faceCubo4">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="1" y="0" z="1"/>
      <ROTATION axis="y" angle="90"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 1 1 0"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="faceCubo5">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="1" z="1"/>
      <ROTATION axis="x" angle="-90"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 1 1 0"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="faceCubo6">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0" z="0"/>
      <ROTATION axis="x" angle="90"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 1 1 0"/>
      </DESCENDANTS>

    </NODE>

    <!-- pyramid -->
    <NODE id="piramide1">

        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="50" y="5.8" z="50" />
        <SCALE sx="0.2" sy="0.9" sz="0.2" />

        <DESCENDANTS>
       		<NODEREF id="meiaPiramide1" />
            <NODEREF id="meiaPiramide2" />
            <NODEREF id="basePiramide" />
        </DESCENDANTS>

    </NODE>

    <NODE id="basePiramide">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

       	<ROTATION axis="x" angle="90" />
       	<TRANSLATION x="1" y="6" z="2.87" />
       	<SCALE sx="2.8" sy="3.5" sz="1" />

        <DESCENDANTS>
					<LEAF  type="rectangle" args="0 4 5 0" />
        </DESCENDANTS>

    </NODE>

    <NODE id="meiaPiramide1">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <DESCENDANTS>
            <NODEREF id="meiaPiramide" />
        </DESCENDANTS>

    </NODE>

    <NODE id="meiaPiramide2">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

		<TRANSLATION x="16" y="0" z="26"	/>
		<ROTATION axis="y" angle="180" />

        <DESCENDANTS>
            <NODEREF id="meiaPiramide" />
        </DESCENDANTS>

    </NODE>

    <NODE id="meiaPiramide">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <DESCENDANTS>
            <NODEREF id="facePiramide1" />
            <NODEREF id="facePiramide2" />
        </DESCENDANTS>

    </NODE>

   <NODE id="facePiramide1">
        <MATERIAL id="greenMaterial" />
        <TEXTURE id="null" />

		<TRANSLATION x="8" y="4" z="13" />
		<ROTATION axis="x" angle="44.42" />
       	<ROTATION axis="y" angle="-45" />

        <DESCENDANTS>
					<LEAF id="triangle1" type="triangle" args="2 0 12 12 0 2 0 0 0" />
        </DESCENDANTS>

    </NODE>

    <NODE id="facePiramide2">
        <MATERIAL id="greenMaterial" />
        <TEXTURE id="null" />

       	<TRANSLATION x="10" y="4" z="15" />
        <TRANSLATION x="-2" y="0" z="-2" />
       	<ROTATION axis="z" angle="-44.42" />
       	<ROTATION axis="y" angle="45" />

        <DESCENDANTS>
					<LEAF id="triangle1" type="triangle" args="2 0 12 12 0 2 0 0 0" />
        </DESCENDANTS>

    </NODE>

  </NODES>

</SCENE>
