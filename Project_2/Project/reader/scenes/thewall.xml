<SCENE>

  <!-- Initial values -->
  <INITIALS>
    <frustum near="0.1" far="500"/>
    <translation x="-3" y="1.5" z="0"/>
    <rotation axis="x" angle="0"/>
    <rotation axis="y" angle="0"/>
    <rotation axis="z" angle="0"/>
    <scale sx="0.05" sy="0.05" sz="0.05"/>
    <reference length="5"/>
  </INITIALS>

  <!-- Illumination and clear colour -->
  <ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1"/>
    <doubleside value="0" />
    <background r="0" g="0" b="0" a="1"/>
  </ILLUMINATION>

  <!-- Lights -->
  <LIGHTS>
    <LIGHT id="light0">
      <enable value="1"/>
      <position x="0" y="100" z="10" w="1"/>
      <ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0"/>
      <specular r="1.0" g="1.0" b="1.0" a="1.0"/>
    </LIGHT>

    <LIGHT id="light1">
      <enable value="1"/>
      <position x="200" y="100" z="10" w="1"/>
      <ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0"/>
      <specular r="1.0" g="1.0" b="1.0" a="1.0"/>
    </LIGHT>

    <LIGHT id="light2">
      <enable value="1"/>
      <position x="-50" y="75" z="100" w="1"/>
      <ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0"/>
      <specular r="1.0" g="1.0" b="1.0" a="1.0"/>
    </LIGHT>

    <LIGHT id="light3">
      <enable value="1"/>
      <position x="250" y="75" z="100" w="1"/>
      <ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
      <diffuse r="0.6" g="0.6" b="0.6" a="1.0"/>
      <specular r="0.6" g="0.6" b="0.6" a="1.0"/>
    </LIGHT>

  </LIGHTS>

  <!-- Textures -->
  <TEXTURES>

    <TEXTURE id="ice">
      <file path="images/ice.jpg"/>
      <amplif_factor s="2" t="2"/>
    </TEXTURE>

    <TEXTURE id="wood">
      <file path="images/wood.png"/>
      <amplif_factor s="1" t="1"/>
    </TEXTURE>

    <TEXTURE id="pine">
      <file path="images/pine.jpg"/>
      <amplif_factor s="15" t="15"/>
    </TEXTURE>

    <TEXTURE id="iron">
      <file path="images/iron.png"/>
      <amplif_factor s="1" t="1"/>
    </TEXTURE>

    <TEXTURE id="snow">
        <file path="images/snow.jpg" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>

  </TEXTURES>

  <MATERIALS>

    <!-- A default material -->
    <MATERIAL id="defaultMaterial">
      <shininess value="1"/>
      <specular r="0.0" g="0.0" b="0.0" a="1"/>
      <diffuse r="0.4" g="0.4" b="0.4" a="1"/>
      <ambient r="0.4" g="0.4" b="0.4" a="1"/>
      <emission r="0.2" g="0.2" b="0.2" a="1"/>
    </MATERIAL>

    <!-- Dark Brown Material -->
    <MATERIAL id="darkBrownMaterial">
      <shininess value="1"/>
      <specular r="0.0" g="0.0" b="0.0" a="1"/>
      <diffuse r="0.3" g="0.09" b="0.00" a="1"/>
      <ambient r="0.0" g="0.0" b="0.0" a="1"/>
      <emission r="0.0" g="0.0" b="0.0" a="1"/>
    </MATERIAL>

  </MATERIALS>

  <ANIMATIONS>

    <ANIMATION id="LinearElevator" speed="2" type="linear">
        <controlpoint xx="0" yy="0" zz="0" /> 
        <controlpoint xx="0" yy="13" zz="0" />
        <controlpoint xx="0" yy="13" zz="-1" />
    </ANIMATION>

    <ANIMATION id="Linear1" speed="5" type="linear">
        <controlpoint xx="0" yy="0" zz="0" /> 
        <controlpoint xx="30" yy="0" zz="0" />
    </ANIMATION>

    <ANIMATION id="Linear2" speed="3" type="linear">
        <controlpoint xx="0" yy="0" zz="0" /> 
        <controlpoint xx="-20" yy="0" zz="0" />
    </ANIMATION>

    <ANIMATION id="Circular1" speed="1" type="circular"
        centerx="0" centery="0" centerz="0"
        radius="2" startang="0" rotang="360" />

    <ANIMATION id="Bezier1" speed="40" type="bezier">
        <controlpoint xx="0" yy="0" zz="0" />
        <controlpoint xx="0" yy="50" zz="0" />
        <controlpoint xx="90" yy="50" zz="0" />
        <controlpoint xx="90" yy="-5" zz="0" />
    </ANIMATION>

    <ANIMATION id="Bezier2" speed="50" type="bezier">
        <controlpoint xx="0" yy="0" zz="0" />
        <controlpoint xx="30" yy="0" zz="0" />
        <controlpoint xx="30" yy="70" zz="0" />
        <controlpoint xx="97" yy="67" zz="-25" />
    </ANIMATION>

    <ANIMATION id="Combo1" type="combo">
        <SPANREF id="Linear1"/>
        <SPANREF id="Bezier1"/>
        <SPANREF id="Bezier2"/>
    </ANIMATION>

  </ANIMATIONS>

  <!-- The leaves -->
  <LEAVES></LEAVES>

  <!-- The nodes -->
  <NODES>

    <ROOT id="root"/>

    <!-- The root node -->
    <NODE id="root">
      <MATERIAL id="defaultMaterial"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="floor"/>
        <NODEREF id="wall"/>
        <NODEREF id="elevator"/>
        <NODEREF id="thewatch"/>
        <NODEREF id="town"/>
        <NODEREF id="forest"/>
        <NODEREF id="snowWave"/>
        <NODEREF id="spheres"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="spheres">
     <MATERIAL id="null"/>
     <TEXTURE id="wood"/>

     <DESCENDANTS>
        <NODEREF id="sphere1"/>
        <NODEREF id="sphere2"/>
      </DESCENDANTS>

   </NODE>

    <NODE id="sphere1">
        <MATERIAL id="null" />
        <TEXTURE id="wood" />

        <TRANSLATION x="90" y="10" z="15"/>

        <DESCENDANTS>
            <LEAF  type="sphere" args="2 20 20" />
        </DESCENDANTS>

    </NODE>

    <NODE id="sphere2">
        <MATERIAL id="null" />
        <TEXTURE id="wood" />

        <ANIMATIONREFS>
          <ANIMATIONREF id="Combo1"/>
        </ANIMATIONREFS>

        <TRANSLATION x="0" y="6" z="20"/>

        <DESCENDANTS>
            <LEAF  type="sphere" args="2 20 20" />
        </DESCENDANTS>

    </NODE>

    <NODE id="floor">
